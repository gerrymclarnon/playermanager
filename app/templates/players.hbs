{{partial "header"}}

<div class="content">
    <div class="card">
        <ul class="table-view">
          {{#each model as |player|}}
            <li class="table-view-cell media">
                {{#link-to "player" player class="navigate-right"}}
                  <img class="media-object pull-left" src="http://placehold.it/42x42">
                  <div class="media-body">
                    {{player.firstName}}
                    <p>{{player.lastName}}</p>
                  </div>
                {{/link-to}}
            </li>
          {{/each}}
        </ul>
    </div>
</div>